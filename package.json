{
  "name": "@revoinc/async-busboy",
  "version": "2.0.4",
  "description": "Promise-based multipart form parser (ESM).",
  "dependencies": {
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.3",
    "busboy": "^1.6.0",
    "standard": "^17.1.0"
  },
  "devDependencies": {
    "mocha": "^10.4.0"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha specs.js --harmony --reporter spec --bail",
    "cover": "NODE_ENV=test istanbul cover -x specs.js _mocha specs.js -- --reporter spec --bail",
    "lint": "prettier --check .",
    "lint-fix": "prettier --write ."
  },
  "type": "module",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revolution-robotics/revoinc-async-busboy.git"
  },
  "standard": {
    "parser": "@babel/eslint-parser"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-top-level-await"
    ]
  },
  "keywords": [
    "koa",
    "multipart",
    "form",
    "file-upload"
  ],
  "author": "Emanuel Chappat <emmanuel.chappat@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/revolution-robotics/revoinc-async-busboy#readme",
  "bugs": {
    "url": "https://github.com/revolution-robotics/revoinc-async-busboy/issues"
  }
